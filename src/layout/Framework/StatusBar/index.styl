.statusBar {
    width: 100%;
    height: 64px;
    background: #ffffff;
    position: relative;
    box-shadow: 0 1px 4px rgba(0, 21, 41, 0.08);

    .openIcon {
        height: 100%;
        margin-left: 10px;
        overflow: hidden;
        display: inline-block;

        :global(.anticon) {
            margin-top: 20px;
            font-size: 24px;
            cursor: pointer;
            margin-left: 10px;
        }
    }

    .handeln {
        float: right;
    }
}